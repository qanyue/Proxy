[Script]
# > 网易云音乐
网易云音乐 = type=cron, cronexp="40 5 0 * * *", wake-system=1, timeout=180, script-path=https://raw.githubusercontent.com/chavyleung/scripts/master/neteasemusic/quanx/neteasemusic.js
# > 滴滴出行
滴滴出行——每日签到 = type=cron,timeout=120,cronexp=05 0 * * *,script-path=https://raw.githubusercontent.com/blackmatrix7/ios_rule_script/master/script/didichuxing/didi_checkin.js
# > 小米运动
小米运动 = type=cron,cronexp="0 50 22 * * *", wake-system=1, timeout=180, script-path=https://raw.githubusercontent.com/lxk0301/jd_scripts/master/backUp/xmSports.js
# > 支付宝
支付宝 = type=cron,cronexp="0 30 7 * * *", wake-system=1, timeout=180, script-path=AliPay.js
# > 互助码
互助码 = type=cron,cronexp="0 30 7 1,10,20 * *", wake-system=1, timeout=180, script-path=ShareCode.js
