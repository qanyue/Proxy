#!name=JD Bean Tasks (Non-Actions)
#!desc=This is a Surge module used to collect JaveScripts on JD which is not uploaded to the script in Tencent Cloud Functions.
#!tag = type=cron, cronexp="cronexample", wake-system=1, timeout=600, script-path=javescript
#!GitHub:https://github.com/chiupam/Proxy/blob/individual/Surge/Non-Actions.sgmodule
#!Raw:https://raw.githubusercontent.com/chiupam/Proxy/individual/Surge/Non-Actions.sgmodule
[Script]
# > 获取京东Cookie
获取京东Cookie = type=http-request, pattern=https:\/\/api\.m\.jd\.com\/client\.action.*functionId=signBean, script-path=https://raw.githubusercontent.com/NobyDa/Script/master/JD-DailyBonus/JD_DailyBonus.js

# > 京东京豆系列
# >>> 京东京豆 {*/12:01}
京豆 = type=cron, cronexp="0 1 */12 * * *", wake-system=1, timeout=600, script-path=https://raw.githubusercontent.com/NobyDa/Script/master/JD-DailyBonus/JD_DailyBonus.js

# >>> 宠汪汪系列
# >>>>> 偷粮 {05:59}
宠汪汪——偷粮 = type=cron,cronexp="55 59 5 * * *", wake-system=1, timeout=600, script-path=https://raw.githubusercontent.com/lxk0301/jd_scripts/master/jd_joy_steal.js

# >>> 京东赚赚 {00:09}
京东赚赚 = type=cron, cronexp="0 0 9 * * *", wake-system=1, timeout=600, script-path=https://raw.githubusercontent.com/whyour/hundun/master/quanx/jdzz.js
京东赚赚获取 Cookie = type=http-request, pattern=^https\:\/\/api\.m\.jd\.com\/client\.action\?functionId\=interactIndex, requires-body=0, max-size=0, script-path=https://raw.githubusercontent.com/whyour/hundun/master/quanx/jdzz.cookie.js

# >>> 东东小窝 {00：11}
东东小窝 = type=cron, cronexp="30 11 0 * * *", wake-system=1, timeout=600, script-path=https://raw.githubusercontent.com/yangtingxiao/QuantumultX/master/scripts/jd/jd_woHome.js

# >>> 领京豆额外奖励 {00:12}
领京豆额外奖励 = type=cron, cronexp="0 12 0 * * *", wake-system=1, timeout=600, script-path=https://raw.githubusercontent.com/lxk0301/jd_scripts/master/jd_bean_home.js

# > 临时活动
# >>> 秒杀红包雨 (2020.12.31结束)
秒杀红包雨 = type=cron, cronexp="0 9 7 * * *", wake-system=1, timeout=600, script-path= https://raw.githubusercontent.com/lxk0301/jd_scripts/master/jd_ms_redrain.js

# > BoxJs TF
BoxJs = type=http-request, pattern=^https?://boxjs.net, script-path=https://gitee.com/chavyleung/scripts/raw/master/box/chavy.boxjs.js, requires-body=true, timeout=120

[URL Rewrite]
^https://boxjs.net http://boxjs.net 302
^http://ww1.boxjs.net http://boxjs.net 302
^http://ww6.boxjs.net http://boxjs.net 302

[MITM]
hostname = %APPEND% api.m.jd.com, boxjs.net

